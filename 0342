# Your code here!
for x in range(1,101):
    for n in range(1,9):
        print(n**10%x,end="\t")
    print("\n")
for x in range(30):
    print(x//2,end=" ")
for x in range(30):
    print(x//2%2,end=" ")
for x in range(30):
    print(x//2%3,end=" ")
for x in range(30):
    print(x//5%3,end=" ")
