for x in range(21):
    print(10-x, end=" ")
for x in range(101):
    print(100-x, end=" ")
for x in range(21):
    print(100-x*10, end=" ")
for x in range(11):
    print(x**2,end=" ")
for x in range(100):
    print(x%5, end=" ")
for x in range(100):
    print(x%2, end=" ")
for x in range(100):
    print((x%2+1)*2,end=" ")


    
